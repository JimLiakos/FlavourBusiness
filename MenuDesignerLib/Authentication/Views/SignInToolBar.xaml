<UserControl x:Class="FLBAuthentication.Views.SignInToolBar"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:FLBAuthentication.Views"
             mc:Ignorable="d" 
             d:DesignHeight="200" d:DesignWidth="300">
    <Grid >
        <Grid  >
            <Border  VerticalAlignment="Stretch" HorizontalAlignment="Stretch" x:Name="WebBrowserHost">
            </Border>
        </Grid>
        <Grid.InputBindings>
            <MouseBinding Gesture="LeftClick" Command="{Binding ClickPseudoCommand}" ></MouseBinding>
        </Grid.InputBindings>

    </Grid>
</UserControl>
