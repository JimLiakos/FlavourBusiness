<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
                    xmlns:sw="clr-namespace:StyleableWindow;assembly=StyleableWindow">

    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source=""/>
    </ResourceDictionary.MergedDictionaries>
    <Style x:Key="PathTrimmingStyle" TargetType="{x:Type sw:PathTrimmingTextBlock}">
        <Setter Property="FontFamily" Value="Segoe UI" />
        <Setter Property="FontSize" Value="16.667" />
        <Setter Property="FontWeight" Value="DemiBold"/>
        <Setter Property="Foreground" Value="{StaticResource NavigationWindowForegroundBrush}" />
    </Style>
</ResourceDictionary>