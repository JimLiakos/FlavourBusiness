<sw:PageDialogViewEmulator  xmlns:sw="clr-namespace:StyleableWindow;assembly=StyleableWindow" x:Class="FLBManager.Views.Preparation.PreparationStationItemsPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:FLBManager.Views.Preparation" 
      xmlns:ConnectableCtrl="clr-namespace:WPFUIElementObjectBind;assembly=WPFConnectableControls" 
      xmlns:ViewModel="clr-namespace:FLBManager.ViewModel.Preparation" 
      xmlns:resx="clr-namespace:FLBManager.Properties"
      mc:Ignorable="d" 
      LanguageButton="True"  
      d:DesignHeight="550" 
      d:DesignWidth="850"

      Title="{x:Static resx:Resources.PreparationStationWindowTitle}" >
    <Page.DataContext>
        <ConnectableCtrl:ObjectContext ViewControlObjectType="{x:Type ViewModel:PreparationStationPresentation}"    Name="Connection" TransactionOption="Required"  />
    </Page.DataContext>

    <Grid>

        <local:PreparationStationItemsView DataContext="{Binding Value, Mode=OneWay}"></local:PreparationStationItemsView>
    </Grid>
</sw:PageDialogViewEmulator>
